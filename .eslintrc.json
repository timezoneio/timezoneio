{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "globals": {
    "require": true,
    "module": true
  },
  "rules": {
    "curly": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "func-names": 0,
    "semi": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0,
    "no-var": 0
  }
}
